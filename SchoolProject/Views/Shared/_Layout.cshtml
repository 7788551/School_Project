    <script src="~/js/jquery.mb.YTPlayer.min.js" asp-append-version="true"></script>
    <script src="~/js/wow.min.js" asp-append-version="true"></script>
    <script src="~/js/function.js" asp-append-version="true"></script>
    
    <script>
                document.documentElement.style.touchAction = "auto";
        document.body.style.touchAction = "auto";

    </script>
    @* @RenderSection("Scripts", false) *@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
    <script>
        $(document).ready(function () {
            if ($('.owl-carousel').length) {
                $('.owl-carousel').owlCarousel({
                    loop: true,
                    margin: 10,
                    nav: true,
                    dots: true,
                    autoplay: true,
                    autoplayTimeout: 2000,
                    smartSpeed: 800,
                    responsive: {
                        0: { items: 1 },
                        576: { items: 2 },
                        768: { items: 3 },
                        992: { items: 4 }
                    }
                });
            } else {
                console.log("❌ Owl carousel container not found");
            }
        });
    </script>

</body>
</html>
